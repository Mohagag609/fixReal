{
  "name": "new-accounting-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:setup": "prisma migrate deploy && npm run db:seed",
    "setup": "npm install && npm run db:push && npm run db:seed",
    "clean": "rm -rf .next node_modules package-lock.json",
    "fresh": "npm run clean && npm run setup",
    "postbuild": "prisma generate",
    "netlify:build": "npm run db:generate && npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "framer-motion": "^12.23.13",
    "glob": "^11.0.3",
    "next": "15.5.3",
    "numeral": "^2.0.6",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.9",
    "lucide-react": "^0.544.0"
  },
  "peerDependencies": {
    "framer-motion": "^12.23.13",
    "lucide-react": "^0.544.0"
  },
  "optionalDependencies": {
    "framer-motion": "^12.23.13",
    "lucide-react": "^0.544.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/numeral": "^2.0.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "framer-motion": "^12.23.13",
    "lucide-react": "^0.544.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
